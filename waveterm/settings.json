{
  "ai:preset": "claude@anthropic",
  "ai:maxtokens": 8192,
  "ai:timeoutms": 120000,
  "app:defaultnewblock": "term",
  "app:dismissarchitecturewarning": true,
  "autoupdate:enabled": true,
  "autoupdate:channel": "latest",
  "autoupdate:installonquit": false,
  "autoupdate:intervalms": 21600000,
  "telemetry:enabled": false,
  "conn:askbeforewshinstall": false,
  "conn:wshenabled": true,
  "conn:wshautoreconnect": true,
  "editor:minimapenabled": true,
  "editor:wordwrap": true,
  "editor:fontsize": 14,
  "editor:tabsize": 2,
  "term:copyonselect": true,
  "term:fontfamily": "Hack Nerd Font Mono",
  "term:fontsize": 13,
  "term:theme": "dracula",
  "term:localshellpath": "/bin/zsh",
  "term:scrollback": 10000,
  "web:defaulturl": "https://github.com/dashboard",
  "web:defaultsearch": "https://duckduckgo.com/?q={query}",
  "web:userdatadir": "~/.config/waveterm/webdata",
  "window:tilegapsize": 6,
  "window:maxtabcachesize": 25,
  "window:nativetitlebar": false,
  "window:confirmclose": false,
  "window:savelastwindow": true,
  "window:showmenubar": false,
  "window:magnifiedblockopacity": 0.8,
  "window:magnifiedblocksize": 0.75,
  "window:magnifiedblockblurprimarypx": 12,
  "window:magnifiedblockblursecondarypx": 4,
  "window:magnifiedblockmargin": 20,
  "tab:preset": "dark@pro",
  "workspace:defaultlayout": "grid",
  "workspace:autosave": true,
  "search:casesensitive": false,
  "search:regex": true,
  "search:globalhistory": true,
  "perf:maxblocks": 50,
  "perf:gcintervalms": 300000,
  "perf:renderthrottle": 60,
  "dev:hotreload": true,
  "dev:debugmode": false,
  "dev:enableexperiments": true,
  "security:allowunsafescripts": false,
  "security:sandboxweb": true,
  "security:encryptlocalstorage": true,
  "notify:enabled": true,
  "notify:position": "top-right",
  "notify:timeout": 5000,
  "keys:globalai": "Ctrl+Space",
  "keys:quicksearch": "Ctrl+Shift+F",
  "keys:newworkspace": "Ctrl+Shift+W",
  "keys:magnifyblock": "Ctrl+M",
  "widgets:enabled": true,
  "widgets:position": "right", 
  "widgets:width": 300,
  "widgets:autohide": false,
  "widgets:showhelp": false,
  "conn:wshmixed": true,
  "conn:wshpersist": true,
  "conn:autoinstall": true,
  "app:splittype": "auto",
  "app:newblockposition": "bottom",
  "block:maxhistory": 5000,
  "block:autocomplete": true,
  "search:multiblock": true,
  "search:includeremote": true,
  "layout:flexgap": true,
  "layout:snap": true,
  "layout:autosave": 30000,
  "preview:maxfilesize": "50MB",
  "preview:autorefresh": true,
  "ui:animations": true,
  "ui:smoothscroll": true,
  "ui:reducedmotion": false
}